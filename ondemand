using System;
using System.Collections.Generic;
using System.Xml.Serialization;

[XmlRoot(ElementName = "FIPSCOLINKFILE")]
public class FipsColinkFile
{
    [XmlElement(ElementName = "DATA")]
    public Data Data { get; set; }

    [XmlAttribute(AttributeName = "VERSION")]
    public string Version { get; set; }
}

public class Data
{
    [XmlElement(ElementName = "AGENTINFORMATION")]
    public AgentInformation AgentInformation { get; set; }

    [XmlElement(ElementName = "PERSON")]
    public List<Person> Persons { get; set; }

    [XmlElement(ElementName = "RELATION")]
    public List<Relation> Relations { get; set; }

    [XmlElement(ElementName = "CASEDATA")]
    public List<CaseData> CaseDataList { get; set; }
}

public class AgentInformation
{
    [XmlElement(ElementName = "FIRSTNAME")]
    public string FirstName { get; set; }

    [XmlElement(ElementName = "MIDDLENAME")]
    public string MiddleName { get; set; }

    [XmlElement(ElementName = "LASTNAME")]
    public string LastName { get; set; }

    [XmlElement(ElementName = "ADDRESS1")]
    public string Address1 { get; set; }

    [XmlElement(ElementName = "ADDRESS2")]
    public string Address2 { get; set; }

    [XmlElement(ElementName = "CITY")]
    public string City { get; set; }

    [XmlElement(ElementName = "STATE")]
    public string State { get; set; }

    [XmlElement(ElementName = "ZIP")]
    public string Zip { get; set; }

    [XmlElement(ElementName = "COUNTRY")]
    public string Country { get; set; }

    [XmlElement(ElementName = "AGENTID")]
    public string AgentId { get; set; }

    [XmlElement(ElementName = "AGENCY")]
    public string Agency { get; set; }

    [XmlElement(ElementName = "GA")]
    public string GA { get; set; }

    [XmlElement(ElementName = "AITXT1")]
    public string AITxt1 { get; set; }

    [XmlElement(ElementName = "AITXT2")]
    public string AITxt2 { get; set; }

    [XmlElement(ElementName = "AITXT3")]
    public string AITxt3 { get; set; }

    [XmlElement(ElementName = "AINUM1")]
    public string AINum1 { get; set; }

    [XmlElement(ElementName = "AINUM2")]
    public string AINum2 { get; set; }

    [XmlElement(ElementName = "AINUM3")]
    public string AINum3 { get; set; }

    [XmlElement(ElementName = "AIDT1")]
    public string AIDT1 { get; set; }

    [XmlElement(ElementName = "AIDT2")]
    public string AIDT2 { get; set; }

    [XmlElement(ElementName = "AIDT3")]
    public string AIDT3 { get; set; }
}

public class Person
{
    [XmlAttribute(AttributeName = "PERSONID")]
    public string PersonId { get; set; }

    [XmlElement(ElementName = "FIRSTNAME")]
    public string FirstName { get; set; }

    [XmlElement(ElementName = "MIDDLEINITIAL")]
    public string MiddleInitial { get; set; }

    [XmlElement(ElementName = "SUFFIX")]
    public string Suffix { get; set; }

    [XmlElement(ElementName = "LASTNAME")]
    public string LastName { get; set; }

    [XmlElement(ElementName = "CLIENTID")]
    public string ClientId { get; set; }

    [XmlElement(ElementName = "MEMBERCOUNCIL")]
    public string MemberCouncil { get; set; }

    [XmlElement(ElementName = "MEMBERSTATUS")]
    public string MemberStatus { get; set; }

    [XmlElement(ElementName = "INACTIVEFEEIND")]
    public string InactiveFeeInd { get; set; }

    [XmlElement(ElementName = "ADDRESS1")]
    public string Address1 { get; set; }

    [XmlElement(ElementName = "ADDRESS2")]
    public string Address2 { get; set; }

    [XmlElement(ElementName = "CITY")]
    public string City { get; set; }

    [XmlElement(ElementName = "STATE")]
    public string State { get; set; }

    [XmlElement(ElementName = "ZIP")]
    public string Zip { get; set; }

    [XmlElement(ElementName = "STATEOFISSUE")]
    public string StateOfIssue { get; set; }

    [XmlElement(ElementName = "DATEOFBIRTH")]
    public string DateOfBirth { get; set; }

    [XmlElement(ElementName = "ISSUEAGE")]
    public string IssueAge { get; set; }

    [XmlElement(ElementName = "SEX")]
    public string Sex { get; set; }

    [XmlElement(ElementName = "PTXT1")]
    public string PTxt1 { get; set; }

    [XmlElement(ElementName = "PTXT2")]
    public string PTxt2 { get; set; }

    [XmlElement(ElementName = "PTXT3")]
    public string PTxt3 { get; set; }

    [XmlElement(ElementName = "PNUM1")]
    public string PNum1 { get; set; }

    [XmlElement(ElementName = "PNUM2")]
    public string PNum2 { get; set; }

    [XmlElement(ElementName = "PNUM3")]
    public string PNum3 { get; set; }

    [XmlElement(ElementName = "PDT1")]
    public string PDT1 { get; set; }

    [XmlElement(ElementName = "PDT2")]
    public string PDT2 { get; set; }

    [XmlElement(ElementName = "PDT3")]
    public string PDT3 { get; set; }
}

