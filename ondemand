function manipulateIframeFields(executionContext) {
    var formContext = executionContext.getFormContext();
    
    // Access the iframe control by its logical name (replace 'IFRAME_myiframe' with your iframe's logical name)
    var iframeControl = formContext.getControl("IFRAME_myiframe");

    if (iframeControl) {
        // Access the HTML iframe element
        var iframeElement = iframeControl.getObject();

        // Poll to check if the iframe content is fully loaded
        var checkIframeLoaded = setInterval(function() {
            var iframeContentWindow = iframeElement.contentWindow;

            if (iframeContentWindow && iframeContentWindow.document) {
                var iframeDocument = iframeContentWindow.document;

                // Check if a specific field in the iframe exists (replace "fieldId" with actual ID of the field inside iframe)
                var fieldToHide = iframeDocument.getElementById("fieldId");

                if (fieldToHide) {
                    // If the field is found, hide it and clear the interval to stop polling
                    fieldToHide.style.display = "none"; // Hide the field
                    clearInterval(checkIframeLoaded);   // Stop polling once the field is found and manipulated
                }
            }
        }, 500);  // Check every 500ms
    } else {
        console.error("Iframe control not found.");
    }
}
