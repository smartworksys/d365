    public string RDT3 { get; set; }
}

public class CaseData
{
    [XmlAttribute(AttributeName = "PERSONID")]
    public string PersonId { get; set; }

    [XmlElement(ElementName = "POLICYNUMBER")]
    public string PolicyNumber { get; set; }

    [XmlElement(ElementName = "PRODUCT")]
    public string Product { get; set; }

    [XmlElement(ElementName = "CLASS")]
    public string Class { get; set; }

    [XmlElement(ElementName = "STATUS")]
    public string Status { get; set; }

    [XmlElement(ElementName = "RATING")]
    public Rating Rating { get; set; }

    [XmlElement(ElementName = "WLFACEOPTION")]
    public string WLFaceOption { get; set; }

    [XmlElement(ElementName = "WLFACEPREMAMT")]
    public decimal WLFacePremAmt { get; set; }

    [XmlElement(ElementName = "WLPREMMODE")]
    public string WLPremMode { get; set; }

    [XmlElement(ElementName = "WLPAYFORM")]
    public string WLPayForm { get; set; }

    [XmlElement(ElementName = "TERMCONVERSION")]
    public string TermConversion { get; set; }

    [XmlElement(ElementName = "GPOEXERCISE")]
    public string GpoExercise { get; set; }

    [XmlElement(ElementName = "PRINT")]
    public string Print { get; set; }

    [XmlElement(ElementName = "ADDLOANINTTOBAL")]
    public string AddLoanIntToBal { get; set; }

    [XmlElement(ElementName = "WDRLTOLOANATCOSTBASIS")]
    public string WdrlToLoanAtCostBasis { get; set; }

    [XmlElement(ElementName = "ISSUEDATE")]
    public string IssueDate { get; set; }

    [XmlElement(ElementName = "PLANVERSIONID")]
    public string PlanVersionId { get; set; }

    [XmlElement(ElementName = "PAYMENTYEARS")]
    public string PaymentYears { get; set; }

    [XmlElement(ElementName = "CTXT3")]
    public string Ctxt3 { get; set; }

    [XmlElement(ElementName = "CNUM1")]
    public string Cnum1 { get; set; }

    [XmlElement(ElementName = "CNUM2")]
    public string Cnum2 { get; set; }

    [XmlElement(ElementName = "CNUM3")]
    public string Cnum3 { get; set; }

    [XmlElement(ElementName = "CDT1")]
    public string CDT1 { get; set; }

    [XmlElement(ElementName = "CDT2")]
    public string CDT2 { get; set; }

    [XmlElement(ElementName = "CDT3")]
    public string CDT3 { get; set; }

    [XmlElement(ElementName = "POLICY")]
    public List<Policy> Policies { get; set; }

    [XmlElement(ElementName = "INFORCE")]
    public Inforce Inforce { get; set; }

    [XmlElement(ElementName = "RIDER")]
    public List<Rider> Riders { get; set; }
}

public class Rating
{
    [XmlElement(ElementName = "TABLE1")]
    public string Table1 { get; set; }

    [XmlElement(ElementName = "FLATEXTRA1")]
    public string FlatExtra1 { get; set; }

    [XmlElement(ElementName = "FLATEXTRA1TOAGE")]
    public string FlatExtra1ToAge { get; set; }

    [XmlElement(ElementName = "PERMANENTFLATEXTRA1")]
    public string PermanentFlatExtra1 { get; set; }

    [XmlElement(ElementName = "TABLE2")]
    public string Table2 { get; set; }

    [XmlElement(ElementName = "FLATEXTRA2")]
    public string FlatExtra2 { get; set; }

    [XmlElement(ElementName = "FLATEXTRA2TOAGE")]
    public string FlatExtra2ToAge { get; set; }

    [XmlElement(ElementName = "PERMANENTFLATEXTRA2")]
    public string PermanentFlatExtra2 { get; set; }
}

public class Policy
{
    [XmlAttribute(AttributeName = "TYPE")]
    public string Type { get; set; }

    [XmlElement(ElementName = "CODE")]
    public string Code { get; set; }
}

