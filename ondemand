using System;
using System.IO;
using System.Xml.Serialization;
using Newtonsoft.Json;

namespace JsonToXmlParser
{
    class Program
    {
        static void Main()
        {
            string jsonFilePath = "path_to_your_json_file.json";
            string jsonData = File.ReadAllText(jsonFilePath);

            // Deserialize JSON into C# objects
            FipsCoLinkFile fipsCoLinkFile = JsonConvert.DeserializeObject<FipsCoLinkFile>(jsonData);

            // Serialize the C# objects into XML
            SerializeToXml(fipsCoLinkFile);
        }

        static void SerializeToXml(FipsCoLinkFile fipsCoLinkFile)
        {
            XmlSerializer xmlSerializer = new XmlSerializer(typeof(FipsCoLinkFile));

            // Remove the default namespaces
            XmlSerializerNamespaces namespaces = new XmlSerializerNamespaces();
            namespaces.Add(string.Empty, string.Empty);

            using (StringWriter textWriter = new StringWriter())
            {
                xmlSerializer.Serialize(textWriter, fipsCoLinkFile, namespaces);
                string xmlOutput = textWriter.ToString();

                // Write the XML output to a file
                File.WriteAllText("output.xml", xmlOutput);
                Console.WriteLine("XML serialization complete. Output saved to output.xml");
            }
        }
    }
}
