function removeCustomFilter(executionContext) {
    var formContext = executionContext.getFormContext();
    var lookupField = formContext.getControl("lookupFieldName"); // Replace with your lookup field schema name

    if (lookupField && formContext.data.entity.attributes.getByName("lookupFieldName").addPreSearchHandler) {
        // Remove the previously added PreSearch event handler
        lookupField.removePreSearch(formContext.data.entity.attributes.getByName("lookupFieldName").addPreSearchHandler);

        // Clear the handler reference
        formContext.data.entity.attributes.getByName("lookupFieldName").addPreSearchHandler = null;
    }
}
