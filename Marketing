function disableSpecificControlInIframe(executionContext) {
    var formContext = executionContext.getFormContext();

    var iframeControl = formContext.getControl("IFRAME_customhtml"); // Replace with your iframe name

    if (!iframeControl) return;

    var iframe = iframeControl.getObject();

    iframe.onload = function () {
        var iframeDocument = iframe.contentWindow.document;

        // Get the element by ID
        var input = iframeDocument.getElementById("mySpecialInput");

        if (input) {
            input.disabled = true;
        }
    };
}
