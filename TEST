Start

// ──────────────────────────────
// REG_TYPE UI Logic
// ──────────────────────────────
IF REG_TYPE == 'M' OR REG_TYPE == 'V':
    Hide State_WH Text Box

// ──────────────────────────────
// REG_TYPE == 'S'
// ──────────────────────────────
IF REG_TYPE == 'S':
    IF Follow_FED == 'R':
        IF FED_WH > 0:
            IF State_WH >= MIN_RATE:
                Save
            ELSE:
                Show Error Message 2
        ELSE:
            
                Hide State_WH Text Box

    ELSE:
        IF REQD_PL_CLS == 'R':
            IF Amount_To == 'Close':
                IF State_WH >= MIN_RATE:
                    Save
                ELSE:
                    Show Error Message 4
            ELSE:
                
                    Hide State_WH Text Box

        ELSE:
            IF REQD_PRM_DIST == 'R':
                IF Member_Age < 59.5:
                    IF State_WH >= MIN_RATE:
                        Save
                    ELSE:
                        Show Error Message 5
                ELSE:
                    
                        Hide State_WH Text Box

            ELSE:
                IF Follow_FED == 'Y':
                    IF FED_WH > 0:
                        IF State_WH >= MIN_RATE:
                            Save
                        ELSE IF State_WH == 0:
                            Show Exemption Radio Button
                            IF Exemption_Received:
                                Save
                            ELSE:
                                Show Error Message 6
                        ELSE:
                            
                                Hide State_WH Text Box

                ELSE:
                    IF REQD_PL_CLS == 'Y':
                        IF Amount_To == 'Close':
                            IF State_WH >= MIN_RATE:
                                Save
                            ELSE IF State_WH == 0:
                                Show Exemption Radio Button
                                IF Exemption_Received:
                                    Save
                                ELSE:
                                    Show Error Message 6
                            ELSE:
                                
                                    Hide State_WH Text Box

                        IF REQD_PRM_DIST == 'Y':
                            IF Member_Age < 59.5:
                                IF State_WH >= MIN_RATE:
                                    Save
                                ELSE IF State_WH == 0:
                                    Show Exemption Radio Button
                                    IF Exemption_Received:
                                        Save
                                    ELSE:
                                        Show Error Message 6
                                ELSE:
                                    
                                        Hide State_WH Text Box

